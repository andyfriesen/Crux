import {
    test(...)
    assert
}

let _ = test("array length", fun() {
    assert.equal(0, len([]))
    assert.equal(1, len([5]))
    assert.equal(2, len(["hi", "bye"]))
})

let _ = test("mutable array length", fun() {
    assert.equal(0, len(mutable []))
    assert.equal(1, len(mutable [5]))
    assert.equal(2, len(mutable ["hi", "bye"]))
})
