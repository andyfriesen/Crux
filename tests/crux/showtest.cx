import test(...)
import assert

test("show basics", () => {
    assert.equal("()", show(()))
    assert.equal("False", show(False))
    assert.equal("True", show(True))
    assert.equal("0", show(0))
    assert.equal("\"hello\"", show("hello"))
    //assert.equal("12.34", show(12.34))
})

test("shown strings are escaped", () => {
    assert.equal("\"\\0\\\\\\\"\"", show("\0\\\""))
})

test("non-printable ascii is escaped", () => {
    assert.equal("\"\\x07\\x17\"", show("\x07\x17"))
})
