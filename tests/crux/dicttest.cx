import dict
import assert
import test(...)

let _ = test("basic dict", () => {
  let d = dict.new()
  assert.equal(None, d->get("hi"))
  d->set("hi", 10)
  assert.equal(Some(10), d->get("hi"))
})

let _ = test("from record", () => {
  let d = dict.from({
    x: "hi",
    y: "bye",
  })
  assert.equal(Some("hi"), d->get("x"))
  assert.equal(Some("bye"), d->get("y"))
  assert.equal(None, d->get("z"))
})
